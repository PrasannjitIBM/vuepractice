{"ast":null,"code":"export default {\n  name: 'ApiCall',\n  data() {\n    return {\n      question: undefined,\n      incorrectAns: undefined,\n      correctAns: undefined,\n      chosenAns: undefined,\n      msg: undefined\n    };\n  },\n  computed: {\n    answers() {\n      let answers = this.incorrectAns && JSON.parse(JSON.stringify(this.incorrectAns));\n      this.incorrectAns && answers.splice(Math.round(Math.random() * answers.length), 0, this.correctAns);\n      return answers;\n    }\n  },\n  created() {\n    this.axios.get(`https://opentdb.com/api.php?amount=1&category=18`).then(response => {\n      this.question = response.data.results[0].question;\n      this.incorrectAns = response.data.results[0].incorrect_answers;\n      this.correctAns = response.data.results[0].correct_answer;\n    });\n  },\n  methods: {\n    submitAns() {\n      if (!this.chosenAns) {\n        this.msg = \"Please select at least one!!!\";\n      } else {\n        if (this.chosenAns === this.correctAns) {\n          this.msg = \"Your Answer is Correct\";\n        } else {\n          this.msg = \"Sorry, wrong Answer!!!\";\n        }\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAeA,eAAe;EACbA,IAAI,EAAE,SAAS;EACfC,IAAI,GAAE;IACJ,OAAM;MACFC,QAAQ,EAAEC,SAAS;MACnBC,YAAY,EAAED,SAAS;MACvBE,UAAU,EAAEF,SAAS;MACrBG,SAAS,EAAEH,SAAS;MACpBI,GAAG,EAAEJ;IACT;EACF,CAAC;EACDK,QAAQ,EAAC;IACPC,OAAO,GAAG;MACN,IAAIA,OAAM,GAAI,IAAI,CAACL,YAAW,IAAKM,IAAI,CAACC,KAAK,CAAED,IAAI,CAACE,SAAS,CAAE,IAAI,CAACR,YAAW,CAAE,CAAG;MACpF,IAAI,CAACA,YAAW,IAAKK,OAAO,CAACI,MAAM,CAAEC,IAAI,CAACC,KAAK,CAAED,IAAI,CAACE,MAAM,EAAC,GAAIP,OAAO,CAACQ,MAAK,CAAG,EAAC,CAAC,EAAC,IAAI,CAACZ,UAAU,CAAC;MACpG,OAAOI,OAAO;IAClB;EACF,CAAC;EACDS,OAAO,GAAE;IACP,IAAI,CAACC,KAAK,CAACC,GAAG,CAAE,kDAAiD,CAAC,CAACC,IAAI,CAAGC,QAAQ,IAAK;MACnF,IAAI,CAACpB,QAAO,GAAIoB,QAAQ,CAACrB,IAAI,CAACsB,OAAO,CAAC,CAAC,CAAC,CAACrB,QAAQ;MACjD,IAAI,CAACE,YAAW,GAAIkB,QAAQ,CAACrB,IAAI,CAACsB,OAAO,CAAC,CAAC,CAAC,CAACC,iBAAiB;MAC9D,IAAI,CAACnB,UAAS,GAAIiB,QA